<?xml version="1.0" encoding="UTF-8"?>
<template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="2cedd86a-cc6c-4ded-a2db-48ad565ab260" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>C++(Linux)</name>
  <settings>
    <options>
      <option name="artifactRules" value="work/log =&gt; logs.zip&#xA;**/hs_err*.log =&gt; crashdumps.zip&#xA;**/core =&gt; crashdumps.zip&#xA;./**/target/rat.txt =&gt; rat.zip&#xA;./dev-tools/IGNITE-*-*.patch =&gt; patch&#xA;/home/teamcity/ignite-startNodes/*.log =&gt; ignite-startNodes.zip" />
      <option name="checkoutMode" value="ON_SERVER" />
      <option name="cleanBuild" value="true" />
      <option name="executionTimeoutMin" value="40" />
    </options>
    <parameters>
      <param name="env.BOOST_TEST_CATCH_SYSTEM_ERRORS" value="no" />
      <param name="env.CPP_STAGING" value="/tmp/cpp_staging" />
      <param name="env.IGNITE_CPP_PRINT_STACK" value="true" />
      <param name="env.IGNITE_HOME" value="%teamcity.build.checkoutDir%" />
      <param name="env.IGNITE_NATIVE_TEST_CLASSPATH" value="true" />
      <param name="env.IGNITE_NATIVE_TEST_CPP_CONFIG_PATH" value="%env.IGNITE_HOME%/modules/platforms/cpp/core-test/config" />
      <param name="env.IGNITE_NATIVE_TEST_CPP_THIN_CONFIG_PATH" value="%env.IGNITE_HOME%/modules/platforms/cpp/thin-client-test/config" />
      <param name="env.IGNITE_NATIVE_TEST_ODBC_CONFIG_PATH" value="%env.IGNITE_HOME%/modules/platforms/cpp/odbc-test/config" />
      <param name="env.LD_LIBRARY_PATH" value="%env.JAVA_HOME%/jre/lib/amd64/server:%env.JAVA_HOME%/lib/server:%env.CPP_STAGING%/lib" />
      <param name="env.ODBCDIR" value="" />
      <param name="env.OPENSSL_HOME" value="/opt/openssl/1.0.2u" />
    </parameters>
    <build-runners />
    <vcs-settings />
    <requirements />
    <build-triggers />
    <build-extensions>
      <extension id="BUILD_EXT_21" type="BuildFailureOnMessage">
        <parameters>
          <param name="buildFailureOnMessage.conditionType" value="matchesRegex" />
          <param name="buildFailureOnMessage.messagePattern" value="&quot;Process exited with error&quot;$" />
          <param name="buildFailureOnMessage.outputText" value="Process exited with error" />
          <param name="buildFailureOnMessage.reverse" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_35" type="BuildFailureOnMessage">
        <parameters>
          <param name="buildFailureOnMessage.conditionType" value="contains" />
          <param name="buildFailureOnMessage.messagePattern" value="memory access violation occurred at address" />
          <param name="buildFailureOnMessage.outputText" value="Memory access violation!" />
          <param name="buildFailureOnMessage.reverse" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_36" type="BuildFailureOnMessage">
        <parameters>
          <param name="buildFailureOnMessage.conditionType" value="contains" />
          <param name="buildFailureOnMessage.messagePattern" value="Detected memory leaks!" />
          <param name="buildFailureOnMessage.outputText" value="Detected memory leaks!" />
          <param name="buildFailureOnMessage.reverse" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_11" type="BuildFailureOnMessage">
        <parameters>
          <param name="buildFailureOnMessage.conditionType" value="contains" />
          <param name="buildFailureOnMessage.messagePattern" value="SEGFAULT" />
          <param name="buildFailureOnMessage.outputText" value="SEGFAULT detected" />
          <param name="buildFailureOnMessage.reverse" value="false" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</template>

