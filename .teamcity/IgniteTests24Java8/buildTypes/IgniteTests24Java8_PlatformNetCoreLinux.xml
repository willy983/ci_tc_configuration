mono Apache.Ignite.Core.Tests/bin/Debug/Apache.Ignite.Core.Tests.exe -basicTests]]></param>
          <param name="teamcity.build.workingDir" value="modules/platforms/dotnet" />
          <param name="teamcity.step.mode" value="default" />
