      <runner id="RUNNER_89" name="Build .NET (Mono)" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[#!/usr/bin/env bash
set -x

#
# Cleanup
#
for dir in bin obj; do
    find ./ -name "${dir}" -exec rm -rfv {} \;
done

#
# Prepare build
#
curl https://api.nuget.org/downloads/nuget.exe -o nuget.exe
mono nuget.exe restore Apache.Ignite.sln
msbuild Apache.Ignite.sln /p:RunCodeAnalysis=false]]></param>
