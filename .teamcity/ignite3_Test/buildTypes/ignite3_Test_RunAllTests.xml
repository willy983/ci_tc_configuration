<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="36c17032-30b6-41f8-a03e-268b0e60d36c" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>--&gt; Run :: All Tests</name>
  <description />
  <settings>
    <options>
      <option name="buildConfigurationType" value="COMPOSITE" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <disabled-settings>
      <setting-ref ref="BUILD_EXT_12" />
      <setting-ref ref="vcsTrigger" />
    </disabled-settings>
    <parameters />
    <build-runners />
    <vcs-settings>
      <vcs-entry-ref root-id="GitHubApacheIgnite3" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:pull/*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
      <build-trigger id="TRIGGER_8" type="schedulingTrigger">
        <parameters>
          <param name="branchFilter" value="+:&lt;default&gt;" />
          <param name="cronExpression_dm" value="*" />
          <param name="cronExpression_dw" value="*" />
          <param name="cronExpression_hour" value="12" />
          <param name="cronExpression_min" value="24" />
          <param name="cronExpression_month" value="*" />
          <param name="cronExpression_sec" value="0" />
          <param name="cronExpression_year" value="*" />
          <param name="dayOfWeek" value="Sunday" />
          <param name="hour" value="0" />
          <param name="minute" value="0" />
          <param name="promoteWatchedBuild" value="true" />
          <param name="revisionRule" value="lastFinished" />
          <param name="revisionRuleBuildBranch" value="+:&lt;default&gt;" />
          <param name="schedulingPolicy" value="daily" />
          <param name="timezone" value="SERVER" />
        </parameters>
        <build-customization enforceCleanCheckoutForDependencies="true">
          <parameters>
            <param name="reverse.dep.*.BUILD_NUMBER" value="%build.number%" />
          </parameters>
        </build-customization>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_12" type="commit-status-publisher">
        <parameters>
          <param name="github_authentication_type" value="token" />
          <param name="github_host" value="https://api.github.com" />
          <param name="publisherId" value="githubStatusPublisher" />
          <param name="secure:github_access_token" value="credentialsJSON:47ba4fe5-5698-4573-92fc-ec69a81d9df4" />
          <param name="vcsRootId" value="GitHubApacheIgnite3" />
        </parameters>
      </extension>
    </build-extensions>
    <dependencies>
      <depend-on sourceBuildTypeId="ignite3_Test_RunUnitTests">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="ignite3_Test_RunSanityChecks">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="ignite3_Test_RunIntegrationTests">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

