<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="03798439-ff72-4060-a173-11d489cdb027" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>--&gt; Run :: Basic Tests</name>
  <description>Runs fastest test suites to get reply as fast as possible. Use -Run All for check changes</description>
  <settings>
    <options>
      <option name="buildConfigurationType" value="COMPOSITE" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <disabled-settings>
      <setting-ref ref="vcsTrigger" />
    </disabled-settings>
    <parameters />
    <build-runners />
    <vcs-settings>
      <vcs-entry-ref root-id="GitHubApacheIgnite" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter"><![CDATA[+:<default>
+:pull/3667/head
+:ignite-2.5]]></param>
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="USE_DEFAULT" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <dependencies>
      <depend-on sourceBuildTypeId="DevAi2x_Basic1">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="DevAi2x_CacheFullApi">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
