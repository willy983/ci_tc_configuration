<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="880be61f-2fb9-4a39-a2a1-b39f410893f7" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>-&gt; Run :: .NET</name>
  <description>Run all .NET related suites</description>
  <settings>
    <options>
      <option name="buildConfigurationType" value="COMPOSITE" />
      <option name="checkoutMode" value="ON_SERVER" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <parameters />
    <build-runners />
    <vcs-settings>
      <vcs-entry-ref root-id="GitHubApacheIgnite" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <dependencies>
      <depend-on sourceBuildTypeId="DevAi2x_PlatformNetInspections">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="DevAi2x_PlatformNet">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="DevAi2x_PlatformNetIntegrations">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="DevAi2x_PlatformNetCoreLinux">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
